<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:p4="http://xamarin.com/mono/android/designer"
    android:background="@drawable/boder"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:id="@+id/tabla_click">

    <Button
        android:background="@drawable/button"
        android:text="SelecciÃ³nar fecha a ingresar"
        android:id="@+id/fecha"
        android:layout_height="26.0dp"
        android:layout_width="314.0dp"
        android:layout_marginBottom="10.5dp"
        android:layout_marginTop="7.5dp"
        android:layout_marginHorizontal="60.0dp"
        android:clickable="false" />
    <TableRow
        android:id="@+id/tableRow1"
        android:gravity="center"
        android:layout_marginBottom="5dp">
        <TextView
            android:text="Desayuno"
            p4:placeholder="true"
            android:id="@+id/textView2" />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnDesayunoAntes"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:background="@drawable/editext"
            android:hint="mg/dl"
            android:gravity="center"
            android:inputType="number"
            android:id="@+id/txt_des_antes"
            android:clickable="false"
            android:editable="false"
            android:enabled="false" 
                />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnDesayunoDespues"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:hint="mg/dl"
            android:gravity="center"
            android:inputType="number"
            android:id="@+id/txt_des_Des"/>
    </TableRow>



    <TableRow
        android:id="@+id/tableRow3"
        android:gravity="center"
        android:layout_marginBottom="5dp">
        <TextView
            android:clickable="false"
            android:text="Almuerzo"
            p4:placeholder="true"
            android:id="@+id/textView8" />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnAlmuerzoAntes"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/txt_alm_antes"
                />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnAlmuerzoDespues"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/txt_alm_des"/>
    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:gravity="center"
        android:layout_marginBottom="5dp">
        <TextView
            android:text="Cena"
            p4:placeholder="true"
            android:id="@+id/textView8" />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnAntesCena"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/Txt_cen_Antes"/>
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnDespuesCena"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/Txt_cen_Despues"/>
    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:gravity="center"
        android:layout_marginBottom="10.0dp">
        <TextView
            android:text="Dormir"
            p4:placeholder="true"
            android:id="@+id/textView8" />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnDormirAntes"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/Txt_Dor_Antes"
                />
        <Button
            android:clickable="false"
            android:background="@drawable/button"
            android:text="Hora"
            android:id="@+id/btnDormirDespues"
            android:layout_marginHorizontal="6dp"
            android:layout_height="20.3dp"
            android:layout_width="50.7dp" />
        <EditText
            android:enabled="false"
            android:editable="false"
            android:clickable="false"
            android:background="@drawable/editext"
            android:inputType="number"
            android:hint="mg/dl"
            android:gravity="center"
            android:id="@+id/Txt_Dor_Despues"/>
    </TableRow>

</TableLayout>


